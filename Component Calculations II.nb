      RowBox[{"RicciScalarcd", "[", "]"}], "->", 
      RowBox[{
       RowBox[{"RicciScalar", "[", "bgcd", "]"}], "[", "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Einsteincd", "[", "inds__", "]"}], "->", 
      RowBox[{
       RowBox[{"Einstein", "[", "bgcd", "]"}], "[", "inds", "]"}]}], ",", 
     "\[IndentingNewLine]", 
