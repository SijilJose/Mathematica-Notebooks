     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Riemanncd", "[", "inds__", "]"}], " ", "->", " ", 
      RowBox[{
       RowBox[{"Riemann", "[", "bgcd", "]"}], "[", "inds", "]"}]}], ",", 
     "\[IndentingNewLine]", 
