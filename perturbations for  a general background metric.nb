        RowBox[{"-", "\[Nu]"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"background", "[", 
       RowBox[{"\[Mu]_Symbol", ",", "\[Nu]_Symbol"}], "]"}], "->", 
      RowBox[{
       RowBox[{"Inv", "[", "backgroundmetric", "]"}], "[", 
       RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"cd", "->", "bgcd"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Riccicd", "[", "inds__", "]"}], "->", 
      RowBox[{
       RowBox[{"Ricci", "[", "bgcd", "]"}], "[", "inds", "]"}]}], ",", 
     "\[IndentingNewLine]", 
