     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Riemanncd", "[", "inds__", "]"}], " ", "->", " ", 
      RowBox[{
       RowBox[{"Riemann", "[", "bgcd", "]"}], "[", "inds", "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"pert", "[", 
       RowBox[{
        RowBox[{"LI", "[", "1", "]"}], ",", "inds__"}], "]"}], "->", 
      RowBox[{"perturbations", "[", "inds", "]"}]}]}], "\[IndentingNewLine]", 
