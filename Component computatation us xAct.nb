Cell[BoxData[
 RowBox[{"a", " ", "=", " ", 
  RowBox[{"2", "*", "G", "*", 
   RowBox[{"mass", "/", 
    RowBox[{"(", 
     RowBox[{"speedc", "*", "speedc"}], ")"}]}]}]}]], "Input",
